{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
    "NEXT_PUBLIC_GOOGLE_PLACES_API_KEY": "@google_places_api_key",
    "NEXT_PUBLIC_API_BASE_URL": "@api_base_url"
  },
  "regions": ["iad1"]
}
